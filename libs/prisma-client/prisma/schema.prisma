generator client {
  provider = "prisma-client-js"
  output   = "../generated"
}

datasource db {
  provider = "postgresql"
  url      = env("MANAGEMENT_API_DATABASE_URL")
}

model User {
  id          Int      @id @default(autoincrement())
  email       String   @unique
  name        String
  profile     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
